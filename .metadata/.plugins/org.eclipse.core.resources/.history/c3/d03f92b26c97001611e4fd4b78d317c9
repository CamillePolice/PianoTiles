package matrice;

public class Main {

	public static void main(String[] args) {
		int longueur = 4;
		int largeur = 5;
		boolean matrice[][] = { {false,false,false,false,false},{false,false,false,false,false} };
		
		boolean [][] tab = new boolean[longueur][largeur];
		
		for(int i=0;i<matrice.length;i++) {
			for(int j=0;i<matrice[i].length;j++) {
				matrice[i][j] = false;
				System.out.println(matrice[i][j]);
			}
		}
		
	}
  }


